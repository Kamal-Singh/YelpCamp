<% include ./partials/header %>
<div class="container">
    <h1 style="text-align: center;">Sign Up!</h1>
    <div style="width: 30%; margin: 0 auto;">
        <form action="/register" method="POST">
        <input class="form-control form-group" type="text" placeholder="Username" name="username" required>
        <input class="form-control form-group" type="password" placeholder="Password" name="password" required>
        <input class="btn btn-lg btn-primary form-group" type="submit" value="Sign Up">
        </form>
    </div>
</div>
<% include ./partials/footer %>